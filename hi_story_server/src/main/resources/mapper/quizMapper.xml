<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="quiz">

	<insert id="createQ" parameterType="qdto">
		insert into quiz
		uid,rname,dname,qname,qcontent1,qcontent2,qcontent3,qcontent4,qcontent5,hcontent,solution,answer
		values
		(#{uid},#{rname},#{dname},#{qname},#{qcontent1},#{qcontent2},#{qcontent3},#{qcontent4},#{qcontent5},#{hcontent],#{solution},#{answer})
	</insert>

    <delete id="deleteQ" parameterType="qdto">
		delete from quiz where uid = #{uid} and qid = #{qid}
	</delete>

	<select id="" resultType="mdto">
    	select uid from member where uname=#{uname} and email=#{email}
    </select>


</mapper>
